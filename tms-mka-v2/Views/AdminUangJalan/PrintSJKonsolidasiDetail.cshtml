        else if (Model.TypeKonsolidasi == "Pallet")
        {
            harga = Model.Pallet.Value;
        }
        else if (Model.TypeKonsolidasi == "Container")
        {
            harga = Model.Container.Value;
        }
        else if (Model.TypeKonsolidasi == "m3")
        {
            harga = Model.m3.Value;
        }
    }
    satuan = satuan == "Tonase" ? "Kg" : satuan;
}
