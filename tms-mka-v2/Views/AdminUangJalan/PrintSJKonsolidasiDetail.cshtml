@{
    string rute = ViewBag.rute;
    string satuan = Model.PerhitunganDasar != "Manual" ? Model.PerhitunganDasar : Model.TypeKonsolidasi;
    var harga = 0;
    
    if(Model.PerhitunganDasar == "Tonase")
    {
        harga = Model.Tonase.Value;
    }
    else if (Model.PerhitunganDasar == "Karton")
    {
        harga = Model.karton.Value;
    }
    else if (Model.PerhitunganDasar == "Pallet")
    {
        harga = Model.Pallet.Value;
    }
    else if (Model.PerhitunganDasar == "Container")
    {
        harga = Model.Container.Value;
    }
    else if (Model.PerhitunganDasar == "m3")
    {
        harga = Model.m3.Value;
    }
    else if (Model.PerhitunganDasar == "Manual")
    {
        if (Model.TypeKonsolidasi == "Tonase")
        {
            harga = Model.Tonase.Value;
        }
        else if (Model.TypeKonsolidasi == "Karton")
        {
            harga = Model.karton.Value;
