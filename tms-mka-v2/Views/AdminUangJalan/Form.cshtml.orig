                window.location.href = "/AdminUangJalan";
            }
            dsBoronganTambahan = new kendo.data.DataSource({
                transport: {
                    read: {
                        url: '/DataBorongan/GetBoronganTambahan?idTruck=' + IdJnsTruck,
                        dataType: "json"
                    },
                },
            });

            gridSpbu = $("#gridSolar").kendoGrid({
                dataSource: {
                    data: dsSolar,
