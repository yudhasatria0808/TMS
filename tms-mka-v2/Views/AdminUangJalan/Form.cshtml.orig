                            //uang
                            var dummyTotal = 0;
                            dsDummy[0].listTfBorongan.forEach(function (item) {
                                var ketemu = false;
                                for (var idx = 0; idx < dsGridUang.length; idx++) {
                                    if(dsGridUang[idx].Nama == item.Nama){
                                        dsGridUang[idx].value = dsGridUang[idx].value + item.value;
